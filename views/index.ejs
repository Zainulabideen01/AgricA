    <!-- About Section -->
    <section id="about" class="py-20 bg-white">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="lg:text-center">
                <h2 class="text-base text-green-600 font-semibold tracking-wide uppercase">About Us</h2>
                <p class="mt-2 text-3xl leading-8 font-extrabold tracking-tight text-gray-900 sm:text-4xl">
                    Our Farming Philosophy
                </p>
            </div>

            <div class="mt-16">
                <div class="grid md:grid-cols-2 gap-8 items-center">
                    <div>
                        <img src="images/FARM/agbe.png" alt="Aerial view of sustainable farm with crop circles and lush green fields" class="rounded-lg shadow-xl">
                    </div>
                    <div>
                        <h3 class="text-2xl font-bold text-green-800 mb-4">Agricultural Practices Since 2020</h3>
                        <p class="text-gray-600 leading-relaxed mb-4">
                            Animasahun has been at the forefront of agricultural production for over 5 years. 
                            Our farm combines traditional wisdom with cutting-edge technology to 
                            produce high-quality crops while preserving the land for future generations.
                        </p>
                        <p class="text-gray-600 leading-relaxed mb-6">
                            We believe in working with nature rather than against it, and our innovative crop rotation systems and water conservation techniques have been recognized by agricultural experts worldwide.
                        </p>
                        <div class="flex flex-wrap gap-4">
                            <div class="flex items-center">
                                <i class="fas fa-check-circle text-green-500 mr-2"></i>
                                <span class="text-gray-700">Certified consultant</span>
                            </div>
                            <div class="flex items-center">
                                <i class="fas fa-check-circle text-green-500 mr-2"></i>
                                <span class="text-gray-700">Sustainable Irrigation</span>
                            </div>
                            <div class="flex items-center">
                                <i class="fas fa-check-circle text-green-500 mr-2"></i>
                                <span class="text-gray-700">Modern fertilization</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Features Section -->
    <section class="py-20 bg-green-50 feature_text">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="lg:text-center">
                <h2 class="text-base text-green-600 font-semibold tracking-wide uppercase">Why Choose Us</h2>
                <p class="mt-2 text-3xl leading-8 font-extrabold tracking-tight text-gray-900 sm:text-4xl">
                   Quality Productions & Innovative Agricultural Solutions
                </p>
            </div>

            <div class="mt-10">
                 <div class="grid md:grid-cols-3 gap-8">
                    <div class="feature-card bg-white p-8 rounded-lg shadow-md transition-all">
                        <div class="flex items-center justify-center h-12 w-12 rounded-md bg-green-100 text-green-600 mb-4">
                            <i class="fas fa-water text-xl"></i>
                        </div>
                        <h3 class="text-lg font-bold text-gray-900 mb-2">Smart Irrigation</h3>
                        <p class="text-gray-600">
                            Our precision irrigation systems use 40% less water while improving crop yields through soil moisture monitoring and automated watering.
                        </p>
                    </div>
                    
                    <div class="feature-card bg-white p-8 rounded-lg shadow-md transition-all">
                        <div class="flex items-center justify-center h-12 w-12 rounded-md bg-green-100 text-green-600 mb-4">
                            <i class="fas fa-seedling text-xl"></i>
                        </div>
                        <h3 class="text-lg font-bold text-gray-900 mb-2">Organic Practices</h3>
                        <p class="text-gray-600">
                            We use only natural pest control methods and organic fertilizers to produce healthier crops without harmful chemicals.
                        </p>
                    </div>
                    
                    <div class="feature-card bg-white p-8 rounded-lg shadow-md transition-all">
                        <div class="flex items-center justify-center h-12 w-12 rounded-md bg-green-100 text-green-600 mb-4">
                            <!-- <i class="fas fa-robot text-xl">H</i> -->
                             <p>H</p>
                        </div>
                        <h3 class="text-lg font-bold text-gray-900 mb-2">Halal Farm Product</h3>
                        <p class="text-gray-600">
                            product are harvested from properly pest controlled crops, with cleans source of funds and trustworthiness.
                        </p>
                    </div>
                    
                    <div class="feature-card bg-white p-8 rounded-lg shadow-md transition-all">
                        <div class="flex items-center justify-center h-12 w-12 rounded-md bg-green-100 text-green-600 mb-4">
                            <i class="fas fa-solar-panel text-xl"></i>
                        </div>
                        <h3 class="text-lg font-bold text-gray-900 mb-2">Five Years Forward: Innovation in Every Seed</h3>
                        <p class="text-gray-600">
                            Our dedicated team has spent over five years researching and 
                            implementing the latest breakthroughs in agricultural science
                        </p>
                    </div>
                    
                    <div class="feature-card bg-white p-8 rounded-lg shadow-md transition-all">
                        <div class="flex items-center justify-center h-12 w-12 rounded-md bg-green-100 text-green-600 mb-4">
                            <i class="fas fa-tractor text-xl"></i>
                        </div>
                        <h3 class="text-lg font-bold text-gray-900 mb-2">Modern Equipment</h3>
                        <p class="text-gray-600">
                            We use the latest farm machinery with GPS guidance and precision planting for maximum efficiency.
                        </p>
                    </div>
                    
                    <div class="feature-card bg-white p-8 rounded-lg shadow-md transition-all">
                        <div class="flex items-center justify-center h-12 w-12 rounded-md bg-green-100 text-green-600 mb-4">
                            <i class="fas fa-truck text-xl"></i>
                        </div>
                        <h3 class="text-lg font-bold text-gray-900 mb-2">Local Distribution</h3>
                        <p class="text-gray-600">
                            Our produce reaches local markets within hours of harvest, ensuring maximum freshness for consumers.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Products Section -->
    <section id="products" class="py-20 bg-white">

            <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="lg:text-center">
                <h2 class="text-base text-green-600 font-semibold tracking-wide uppercase">Our Harvest</h2>
                <p class="mt-2 text-3xl leading-8 font-extrabold tracking-tight text-gray-900 sm:text-4xl">
                    Farm Fresh Products
                </p>
            </div>
            <div class="mt-12">
                    <div class="grid md:grid-cols-3 gap-8">
                     <% HomePictures.forEach(product => { %>

                        <div class="product-card bg-white rounded-xl shadow-lg overflow-hidden transition-all hover:shadow-2xl hover:-translate-y-1">
                            <div class="h-64 md:h-72 lg:h-80 w-full overflow-hidden bg-gray-100">
                         <% if (product.images && product.images.length > 0) { %>

                                <img src="<%= product.images[0] %>" alt="<%= product.name %>" class="w-full h-full object-cover hover:scale-105 transition-all duration-500">

                            <% } %>
                          
                            </div>
                            <div class="p-6">
                                <h3 class="text-xl font-bold text-gray-900 mb-2"><%= product.name %></h3>
                                <p class="text-gray-600 mb-4"><%= product.description %></p>
                                <div class="flex justify-between items-center">
                                    <a href="/product/<%= product.slug %>" class="text-green-600 hover:underline">
                                    <button>Read More</button>
                                    </a>
                                </div>
                            </div>
                        </div>

                       <% }) %>
                     

                    </div>
                <div class="mt-12 text-center">
                <a href="/productSection" class="inline-flex items-center px-6 py-3 border border-transparent text-base font-medium rounded-md text-white bg-green-600 hover:bg-green-700 transition-all">
                        View All Products
                        <i class="fas fa-arrow-right ml-2"></i>
                    </a>
                </div>

            </div>
           </div>


    </section>

