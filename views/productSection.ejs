<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0, minimum-scale=1.0">
    <title>Animasahun Agricultural Product</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <link rel="stylesheet" href="css/style.css">
     <link rel="shortcut icon" href="images/favicon_io/favicon.ico" type="image/x-icon">
</head>
<body class="bg-gray-50">
    <!-- Navigation -->
    <nav class="fixed w-full bg-white shadow-md z-50">

        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="flex justify-between h-16 items-center">
                <div class="flex items-center">
                    <div class="flex-shrink-0 flex items-center">
                        <i class="fas fa-leaf text-green-600 text-2xl mr-2"></i>
                        <span class="text-xl font-bold text-green-800">Animasahun</span>
                    </div>
                </div>
                <div class="hidden md:block">
                    <div class="ml-10 flex items-center space-x-4">
                        <a href="/" class="text-gray-700 hover:text-green-600 px-3 py-2 rounded-md text-sm font-medium transition-all">Home</a>
                        <a href="#services" class="text-gray-700 active hover:text-green-600 px-3 py-2 rounded-md text-sm font-medium transition-all">Services</a>
                        <a href="/productSection" class="text-green-800 hover:text-green-600 px-3 py-2 rounded-md text-sm font-medium transition-all">Products</a>
                        <a href="#contact" class="text-gray-700 hover:text-green-600 px-3 py-2 rounded-md text-sm font-medium transition-all">Contact</a>
                    </div>
                </div>
                <div class="-mr-2 flex md:hidden">
                    <button type="button" id="mobile-menu-button" class="inline-flex items-center justify-center p-2 rounded-md text-gray-700 hover:text-green-600 hover:bg-gray-100 focus:outline-none">
                        <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
                        </svg>
                    </button>
                </div>
            </div>
        </div>
        
        <!-- Mobile menu -->
        <div class="hidden md:hidden bg-white shadow-lg" id="mobile-menu">
            <div class="px-2 pt-2 pb-3 space-y-1 sm:px-3">
                <a href="/" class="text-gray-700 hover:text-green-600 block px-3 py-2 rounded-md text-base font-medium">Home</a>
                <a href="#services" class="text-gray-700 hover:text-green-600 block px-3 py-2 rounded-md text-base font-medium">Services</a>
                <a href="/productSection" class="text-green-800 hover:text-green-600 block px-3 py-2 rounded-md text-base font-medium">Products</a>
                <a href="#contact" class="text-gray-700 hover:text-green-600 block px-3 py-2 rounded-md text-base font-medium">Contact</a>
            </div>
        </div>
    </nav>

    <!-- Products Section -->
    <section id="products" class="py-20 bg-white">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="lg:text-center">
                <h2 class="text-base text-green-600 font-semibold tracking-wide uppercase">Our Harvest</h2>
                <p class="mt-2 text-3xl leading-8 font-extrabold tracking-tight text-gray-900 sm:text-4xl">
                    Farm Fresh Products
                </p>
            </div>

            <div class="mt-12">
  
                
                 <div class="grid md:grid-cols-3 gap-8">
                     <% OurPictures.forEach(product => { %>

                        <div class="product-card bg-white rounded-xl shadow-lg overflow-hidden transition-all hover:shadow-2xl hover:-translate-y-1">
                            <div class="h-64 md:h-72 lg:h-80 w-full overflow-hidden bg-gray-100">
                         <% if (product.images && product.images.length > 0) { %>

                                <img src="<%= product.images[0] %>" alt="<%= product.name %>" class="w-full h-full object-cover hover:scale-105 transition-all duration-500">

                            <% } %>
                          
                            </div>
                            <div class="p-6">
                                <h3 class="text-xl font-bold text-gray-900 mb-2"><%= product.name %></h3>
                                <p class="text-gray-600 mb-4 first_Description"><%= product.description %></p>
                                <div class="flex justify-between items-center">
                                    <a href="/product/<%= product.slug %>" class="text-green-600 hover:underline">
                                    <button>Read More</button>
                                    </a>
                                </div>
                            </div>
                        </div>

                       <% }) %>
                     

                    </div>
            </div>
        </div>
        </div>
    </section>

      <!-- Services Section -->
    <section id="services" class="py-20 bg-green-800 text-white">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="lg:text-center">
                <h2 class="text-base text-green-300 font-semibold tracking-wide uppercase">What We Offer</h2>
                <p class="mt-2 text-3xl leading-8 font-extrabold tracking-tight sm:text-4xl">
                    Our Farming Services
                </p>
            </div>

            <div class="mt-12">
                <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-8">
                    <div class="bg-green-700 p-6 rounded-lg">
                        <div class="flex items-center mb-4">
                            <i class="fas fa-tractor text-green-300 text-2xl mr-3"></i>
                            <h3 class="text-xl font-bold">Farm Consulting</h3>
                        </div>
                        <p class="text-green-100">
                            Our agricultural experts provide customized consulting for small and large farms looking to improve yields or transition to organic practices.
                        </p>
                    </div>
                    
                    <div class="bg-green-700 p-6 rounded-lg">
                        <div class="flex items-center mb-4">
                            <i class="fas fa-seedling text-green-300 text-2xl mr-3"></i>
                            <h3 class="text-xl font-bold">Soil Analysis</h3>
                        </div>
                        <p class="text-green-100">
                            Comprehensive soil testing and amendment recommendations to optimize fertility and structure for your specific crops.
                        </p>
                    </div>
                    
                    <div class="bg-green-700 p-6 rounded-lg">
                        <div class="flex items-center mb-4">
                            <i class="fas fa-water text-green-300 text-2xl mr-3"></i>
                            <h3 class="text-xl font-bold">Irrigation Installation</h3>
                        </div>
                        <p class="text-green-100">
                            Design and installation of water-efficient drip and sprinkler systems tailored to your land and crops.
                        </p>
                    </div>
                    
                    <div class="bg-green-700 p-6 rounded-lg">
                        <div class="flex items-center mb-4">
                            <i class="fas fa-cow text-green-300 text-2xl mr-3"></i>
                            <h3 class="text-xl font-bold">Livestock Management</h3>
                        </div>
                        <p class="text-green-100">
                            Holistic grazing plans and livestock health services for herds of all sizes using humane, natural methods.
                        </p>
                    </div>
                    
                    <div class="bg-green-700 p-6 rounded-lg">
                        <div class="flex items-center mb-4">
                            <i class="fas fa-leaf text-green-300 text-2xl mr-3"></i>
                            <h3 class="text-xl font-bold">Organic Certification</h3>
                        </div>
                        <p class="text-green-100">
                            Guidance through the entire organic certification process with USDA-accredited specialists.
                        </p>
                    </div>
                    
                    <div class="bg-green-700 p-6 rounded-lg">
                        <div class="flex items-center mb-4">
                            <i class="fas fa-sun text-green-300 text-2xl mr-3"></i>
                            <h3 class="text-xl font-bold">Farm Tours & Education</h3>
                        </div>
                        <p class="text-green-100">
                            On-site educational programs for schools, families, and professional farmers showcasing sustainable practices.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Contact Section -->
    <section id="contact" class="py-20 bg-white">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="grid md:grid-cols-2 gap-12 items-center">
                <div>
                    <h2 class="text-3xl font-bold text-gray-900 mb-6">Get In Touch</h2>
                    <p class="text-gray-600 mb-8">
                        Have questions about our farming methods or products? Interested in a farm tour or consulting services? We'd love to hear from you!
                    </p>
                    
                    <div class="space-y-4">
                        <div class="flex items-start">
                            <div class="flex-shrink-0 h-6 w-6 text-green-600 mt-1">
                                <i class="fas fa-map-marker-alt"></i>
                            </div>
                            <div class="ml-3">
                                <h3 class="text-sm font-bold text-gray-900">Farm Address</h3>
                                <p class="text-sm text-gray-600">1248 Ogbooro Road, Sango saki</p>
                            </div>
                        </div>
                        
                        <div class="flex items-start">
                            <div class="flex-shrink-0 h-6 w-6 text-green-600 mt-1">
                                <i class="fas fa-phone-alt"></i>
                            </div>
                            <div class="ml-3">
                                <h3 class="text-sm font-bold text-gray-900">Call Us</h3>
                                <p class="text-sm text-gray-600">+234-9077-668-850</p>
                            </div>
                        </div>
                        
                        <div class="flex items-start">
                            <div class="flex-shrink-0 h-6 w-6 text-green-600 mt-1">
                                <i class="fas fa-envelope"></i>
                            </div>
                            <div class="ml-3">
                                <h3 class="text-sm font-bold text-gray-900">Email Us</h3>
                                <p class="text-sm text-gray-600">info@Animasahun.com</p>
                            </div>
                        </div>
                        
                        <div class="flex items-start">
                            <div class="flex-shrink-0 h-6 w-6 text-green-600 mt-1">
                                <i class="fas fa-clock"></i>
                            </div>
                            <div class="ml-3">
                                <h3 class="text-sm font-bold text-gray-900">Farm Stand Hours</h3>
                                <p class="text-sm text-gray-600">Monday-Saturday: 9AM-5PM</p>
                            </div>
                        </div>
                    </div>
                    
                    <div class="mt-8">
                        <h3 class="text-lg font-bold text-gray-900 mb-4">Follow Us</h3>
                        <div class="flex space-x-4">
                            <a href="#" class="h-10 w-10 rounded-full bg-green-100 flex items-center justify-center text-green-600 hover:bg-green-200 transition-all">
                                <i class="fab fa-facebook-f"></i>
                            </a>
                            <a href="#" class="h-10 w-10 rounded-full bg-green-100 flex items-center justify-center text-green-600 hover:bg-green-200 transition-all">
                                <i class="fab fa-instagram"></i>
                            </a>
                            <a href="#" class="h-10 w-10 rounded-full bg-green-100 flex items-center justify-center text-green-600 hover:bg-green-200 transition-all">
                                <i class="fab fa-twitter"></i>
                            </a>
                            <a href="#" class="h-10 w-10 rounded-full bg-green-100 flex items-center justify-center text-green-600 hover:bg-green-200 transition-all">
                                <i class="fab fa-whatsapp"></i>
                            </a>
                        </div>
                    </div>
                </div>
                
                <div>
                    <div class="bg-white shadow-lg rounded-lg p-8">
                        <form id="contactForm">
                            <div class="grid grid-cols-1 gap-6">
                                <div>
                                    <label for="name" class="block text-sm font-medium text-gray-700 mb-1">Your Name</label>
                                    <input type="text" id="name" name="name" class="w-full px-4 py-2 border border-gray-300 rounded-md focus:ring-green-500 focus:border-green-500" required>
                                </div>
                                
                                <div>
                                    <label for="email" class="block text-sm font-medium text-gray-700 mb-1">Email Address</label>
                                    <input type="email" id="email" name="email" class="w-full px-4 py-2 border border-gray-300 rounded-md focus:ring-green-500 focus:border-green-500" required>
                                </div>
                                
                                <div>
                                    <label for="phone" class="block text-sm font-medium text-gray-700 mb-1">Phone Number</label>
                                    <input type="tel" id="phone" name="phone" class="w-full px-4 py-2 border border-gray-300 rounded-md focus:ring-green-500 focus:border-green-500">
                                </div>
                                
                                <div>
                                    <label for="subject" class="block text-sm font-medium text-gray-700 mb-1">Subject</label>
                                    <select id="subject" name="subject" class="w-full px-4 py-2 border border-gray-300 rounded-md focus:ring-green-500 focus:border-green-500">
                                        <option value="general">General Inquiry</option>
                                        <option value="farm-tour">Farm Tour</option>
                                        <option value="consulting">Consulting Services</option>
                                        <option value="wholesale">Wholesale Order</option>
                                        <option value="other">Other</option>
                                    </select>
                                </div>
                                
                                <div>
                                    <label for="message" class="block text-sm font-medium text-gray-700 mb-1">Message</label>
                                    <textarea id="message" name="message" rows="4" class="w-full px-4 py-2 border border-gray-300 rounded-md focus:ring-green-500 focus:border-green-500"></textarea>
                                </div>
                                
                                <div>
                                    <button type="submit" class="w-full px-6 py-3 border border-transparent text-base font-medium rounded-md text-white bg-green-600 hover:bg-green-700 transition-all">
                                        Send Message
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- Footer -->
    <footer class="bg-green-900 text-white">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="grid md:grid-cols-4 gap-12 py-12">
                <div>
                    <div class="flex items-center mb-4">
                        <i class="fas fa-leaf text-green-400 text-2xl mr-2"></i>
                        <span class="text-xl font-bold">Animasahun</span>
                    </div>
                    <p class="text-green-200 text-sm">
                        Sustainable farming solutions for a healthier planet and more nutritious food.
                    </p>
                    <div class="mt-4 flex space-x-4">
                        <a href="#" class="text-green-300 hover:text-white">
                            <i class="fab fa-facebook-f"></i>
                        </a>
                        <a href="#" class="text-green-300 hover:text-white">
                            <i class="fab fa-instagram"></i>
                        </a>
                        <a href="#" class="text-green-300 hover:text-white">
                            <i class="fab fa-twitter"></i>
                        </a>
                        <a href="#" class="text-green-300 hover:text-white">
                            <i class="fab fa-whatsapp"></i>
                        </a>
                    </div>
                </div>
                
                <div>
                    <h3 class="text-lg font-bold mb-4">Quick Links</h3>
                    <ul class="space-y-2">
                        <li><a href="/" class="text-green-200 hover:text-white transition-all">Home</a></li>
                        <li><a href="#services" class="text-green-200 hover:text-white transition-all">Services</a></li>
                        <li><a href="#products" class="text-green-200 hover:text-white transition-all">Products</a></li>
                        <li><a href="#contact" class="text-green-200 hover:text-white transition-all">Contact</a></li>
                    </ul>
                </div>
                
                <div>
                    <h3 class="text-lg font-bold mb-4">Services</h3>
                    <ul class="space-y-2">
                        <li><a href="#" class="text-green-200 hover:text-white transition-all">Farm Consulting</a></li>
                        <li><a href="#" class="text-green-200 hover:text-white transition-all">Soil Analysis</a></li>
                        <li><a href="#" class="text-green-200 hover:text-white transition-all">Irrigation Installation</a></li>
                        <li><a href="#" class="text-green-200 hover:text-white transition-all">Livestock Management</a></li>
                        <li><a href="#" class="text-green-200 hover:text-white transition-all">Farm Tours</a></li>
                    </ul>
                </div>
                
                <!-- <div>
                    <h3 class="text-lg font-bold mb-4">Newsletter</h3>
                    <p class="text-green-200 text-sm mb-4">
                        Subscribe to our newsletter for farming tips, seasonal recipes, and farm events.
                    </p>
                    <form class="flex">
                        <input type="email" placeholder="Your email" class="px-4 py-2 w-full rounded-l-md focus:outline-none text-gray-900">
                        <button type="submit" class="bg-green-600 hover:bg-green-500 px-4 py-2 rounded-r-md transition-all">
                            <i class="fas fa-paper-plane"></i>
                        </button>
                    </form>
                </div> -->
            
            </div>
            
            <div class="border-t border-green-800 py-6">
                <div class="flex flex-col md:flex-row justify-between items-center">
                    <p class="text-green-300 text-sm mb-4 md:mb-0">
                        © 2025 Animasahun. All rights reserved.
                    </p>
                    <div class="flex space-x-6">
                        <a href="#" class="text-green-300 hover:text-white text-sm transition-all">Privacy Policy</a>
                        <a href="#" class="text-green-300 hover:text-white text-sm transition-all">Terms of Service</a>
                        <a href="#" class="text-green-300 hover:text-white text-sm transition-all">Accessibility</a>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- Back to Top Button -->
    <a href="/productSection" id="back-to-top" class="fixed bottom-4 right-4 bg-green-600 text-white p-3 rounded-full shadow-lg opacity-0 invisible transition-all">
        <i class="fas fa-arrow-up"></i>
    </a>
    <script src="js/script.js"></script>
</body>
</html>